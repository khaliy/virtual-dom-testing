{
    "rules": {
        "semi": [2, "always"],
        "quotes": [2, "single"],
        "strict": [2, "function"],
        "no-eval": 2
    },
    "env": {
        "browser": true,
        "node": true
    },
    "globals": {
        "define": true,
        "document": true,
        "assert": true,
        "Promise": true
    }
}